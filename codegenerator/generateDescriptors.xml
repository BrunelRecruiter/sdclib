    	<include path = "OSCLib/Data/OSCP/MDIB/Base64Binary.h" />
    	<include path = "OSCLib/Data/OSCP/MDIB/DicomNetworkAe.h" />
    	<include path = "OSCLib/Data/OSCP/MDIB/DicomNetworkConnection.h" />
    	<include path = "OSCLib/Data/OSCP/MDIB/LocalizedText.h" />
    	<propertyList nameSingle = "NetworkAe" nameSeveral = "NetworkAes" type = "DicomNetworkAe" />
    	<propertyList nameSingle = "NetworkConnection" nameSeveral = "NetworkConnection" type = "DicomNetworkConnection" />
    	<propertyList nameSingle = "SoftwareVersion" nameSeveral = "SoftwareVersions" type = "std::string" />
    	<propertyList nameSingle = "InstitutionName" nameSeveral = "InstitutionNames" type = "LocalizedText" />
    	<propertyList nameSingle = "InstitutionAddress" nameSeveral = "InstitutionAddresses" type = "LocalizedText" />
    	<propertyList nameSingle = "InstitutionalDepartmentName" nameSeveral = "InstitutionalDepartmentNames" type = "LocalizedText" />
    	<propertyList nameSingle = "PrimaryDeviceType" nameSeveral = "PrimaryDeviceTypes" type = "LocalizedText" />
    	<propertyList nameSingle = "PublicCertificate" nameSeveral = "PublicCertificates" type = "Base64Binary" />
		<property name = "Description" type = "std::string" optional = "true" />
		<property name = "StationName" type = "std::string" optional = "true" />
		<property name = "IssuerOfPatientID" type = "std::string" optional = "true" />
    </class>

    <class name = "EnsembleContextDescriptor" parent = "AbstractContextDescriptor" abstract = "false" >
    	<typedef name = "StateType" type = "EnsembleContextState" />
    </class>

    <class name = "EnumStringMetricDescriptor" parent = "StringMetricDescriptor" abstract = "false" >
	<propertyList nameSingle = "AllowedValue" nameSeveral = "AllowedValues" type = "AllowedValue" />
    </class>

    <class name = "AllowedValue" parent = "NULL" abstract = "false" >
    	<include path = "OSCLib/Data/OSCP/MDIB/CodedValue.h" />
	<property name = "Value" type = "std::string" optional = "false" />
	<property name = "Type" type = "CodedValue" optional = "true" />
    </class>
	
    <class name = "MdsDescriptor" parent = "AbstractAlertingDeviceComponentDescriptor" abstract = "false" >
    	<include path = "OSCLib/Data/OSCP/MDIB/AlertSystemDescriptor.h" />
    	<include path = "OSCLib/Data/OSCP/MDIB/ScoDescriptor.h" />
    	<include path = "OSCLib/Data/OSCP/MDIB/VmdDescriptor.h" />
    	<include path = "OSCLib/Data/OSCP/MDIB/ClockDescriptor.h" />
    	<include path = "OSCLib/Data/OSCP/MDIB/SystemContextDescriptor.h" />
	<include path = "OSCLib/Data/OSCP/MDIB/MetaData.h" />
	<include path = "OSCLib/Data/OSCP/MDIB/BatteryDescriptor.h" />
	<property name = "MetaData" type = "MetaData" optional = "true" />
	<property name = "Context" type = "SystemContextDescriptor" optional = "false" />
	<property name = "Clock" type = "ClockDescriptor" optional = "true" />
    	<property name = "Sco" type = "ScoDescriptor" optional = "true" />
    	<property name = "Battery" type = "BatteryDescriptor" optional = "true" />
    	<propertyList nameSingle = "VMD" nameSeveral = "VMDs" type = "VmdDescriptor" />
    </class>

    <class name = "BatteryDescriptor" parent = "AbstractDeviceComponentDescriptor" abstract = "false" >
    	<include path = "OSCLib/Data/OSCP/MDIB/Measurement.h" />
	<property name = "CapacityFullCharge" type = "Measurement" optional = "true" />
	<property name = "CapacitySpecified" type = "Measurement" optional = "true" />
	<property name = "VoltageSpecified" type = "Measurement" optional = "true" />
    </class>

    <class name = "Measurement" parent = "AbstractDeviceComponentDescriptor" abstract = "false" >
