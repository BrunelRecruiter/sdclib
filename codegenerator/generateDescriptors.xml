<?xml version="1.0"?>
<classes script = "generate.gsl" >

    <class name = "AbstractAlertDescriptor" parent = "AbstractDescriptor" abstract = "true" >
    </class>

    <class name = "AbstractDescriptor" parent = "NULL" abstract = "true" >
        <include path = "OSCLib/Data/OSCP/MDIB/CodedValue.h" />
        <include path = "OSCLib/Data/OSCP/MDIB/VersionCounter.h" />
    	<property name = "Type" type = "CodedValue" optional = "true" />
        <property name = "Handle" type = "std::string" optional = "false" />
        <property name = "DescriptorVersion" type = "VersionCounter" optional = "true" />
        <property name = "SafetyClassification" type = "SafetyClassification" optional = "true" />
    </class>
    
    <class name = "AbstractDeviceComponentDescriptor" parent = "AbstractDescriptor" abstract = "true" >
    	<include path = "OSCLib/Data/OSCP/MDIB/ProductionSpecification.h" />
    	<propertyList nameSingle = "ProductionSpecification" nameSeveral = "ProductionSpecifications" type = "ProductionSpecification" />
    </class>

    <class name = "AbstractContextDescriptor" parent = "AbstractDescriptor" abstract = "true" >
    </class>

    <class name = "AbstractAlertingDeviceComponentDescriptor" parent = "AbstractDeviceComponentDescriptor" abstract = "true" >
		<include path = "OSCLib/Data/OSCP/MDIB/AlertSystemDescriptor.h" />
    	<property name = "AlertSystem" type = "AlertSystemDescriptor" optional = "true" />
    </class>

    <class name = "AbstractMetricDescriptor" parent = "AbstractDescriptor" abstract = "true" >
    	<include path = "OSCLib/Data/OSCP/MDIB/CodedValue.h" />
    	<include path = "OSCLib/Data/OSCP/MDIB/Duration.h" />
    	<property name = "Unit" type = "CodedValue" optional = "false" />
		<propertyList nameSingle = "BodySite" nameSeveral = "BodySites" type = "CodedValue" />
    	<property name = "MetricCategory" type = "MetricCategory" optional = "false" />
    	<property name = "Availability" type = "MetricAvailability" optional = "false" />
    	<property name = "MaxDelayTime" type = "Duration" optional = "true" />
    </class>
    
    <class name = "AbstractOperationDescriptor" parent = "AbstractDescriptor" abstract = "true" >
        <property name = "OperationTarget" type = "std::string" optional = "false" />
    </class>

    <class name = "AbstractSetStateOperationDescriptor" parent = "AbstractOperationDescriptor" abstract = "true" >
	<propertyList nameSingle = "ModifiableElement" nameSeveral = "ModifiableElements" type = "std::string" />
    </class>
    
    <class name = "ActivateOperationDescriptor" parent = "AbstractOperationDescriptor" abstract = "false" >
    	<include path = "OSCLib/Data/OSCP/MDIB/Duration.h" />
    	<property name = "ActivationDuration" type = "Duration" optional = "true" />
    </class>    

    <class name = "AlertConditionDescriptor" parent = "AbstractAlertDescriptor" abstract = "false" >
    	<include path = "OSCLib/Data/OSCP/MDIB/CauseInfo.h" />
        <include path = "OSCLib/Data/OSCP/MDIB/CodedValue.h" />
        <property name = "Kind" type = "AlertConditionKind" optional = "false" />
        <propertyList nameSingle = "Source" nameSeveral = "Sources" type = "std::string" />
        <property name = "Priority" type = "AlertConditionPriority" optional = "false" />
        <propertyList nameSingle = "CauseInfo" nameSeveral = "CauseInfo" type = "CauseInfo" />
    </class>

    <class name = "AlertSignalDescriptor" parent = "AbstractAlertDescriptor" abstract = "false" >
        <include path = "OSCLib/Data/OSCP/MDIB/Duration.h" />
        <property name = "ConditionSignaled" type = "std::string" optional = "false" />
        <property name = "Manifestation" type = "AlertSignalManifestation" optional = "false" />
        <property name = "Latching" type = "bool" optional = "false" />
        <property name = "DefaultSignalGenerationDelay" type = "Duration" optional = "true" />
        <property name = "SignalDelegationSupported" type = "bool" optional = "true" />
        <property name = "AcknowledgementSupported" type = "bool" optional = "true" />
		<property name = "AcknowledgeTimeout" type = "Duration" optional = "true" />
    </class>

    <class name = "AlertSystemDescriptor" parent = "AbstractAlertDescriptor" abstract = "false" customImpl = "true">
    	<include path = "OSCLib/Data/OSCP/MDIB/AlertConditionDescriptor.h" />
    	<include path = "OSCLib/Data/OSCP/MDIB/AlertSignalDescriptor.h" />
    	<include path = "OSCLib/Data/OSCP/MDIB/Duration.h" />
    	<propertyList nameSingle = "AlertSignal" nameSeveral = "AlertSignals" type = "AlertSignalDescriptor" />
    	<property name = "MaxPhysiologicalParallelAlarms" type = "unsigned int" optional = "true" />
    	<property name = "MaxTechnicalParallelAlarms" type = "unsigned int" optional = "true" />
    	<property name = "SelfCheckPeriod" type = "Duration" optional = "true" />
    </class>
    
    <class name = "ChannelDescriptor" parent = "AbstractDeviceComponent" abstract = "false" customImpl = "true" >
		<include path = "OSCLib/Data/OSCP/MDIB/AlertSystemDescriptor.h" />
    	<property name = "AlertSystem" type = "AlertSystemDescriptor" optional = "true" />
    </class>

    <class name = "ClockDescriptor" parent = "AbstractDescriptor" abstract = "false" >
   		<include path = "OSCLib/Data/OSCP/MDIB/CodedValue.h" />
